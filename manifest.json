{
  "manifest_version": 2,
  "name": "表单模板助手",
  "version": "1.0",
  "description": "保存和加载网页表单模板，快速填充输入框。",
  "permissions": ["activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}    